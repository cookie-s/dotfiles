[user]
	email = kcz@kcz.sh
	name = cookie-s
	#signingkey = ###
[push]
	default = tracking
	autoSetupRemote = true
[pull]
	ff = only
[core]
	editor = nvim
	excludesfile = ~/.gitignore_global
[http]
	sslVerify = true
[gpg]
	program = gpg
[commit]
	#gpgsign = true
[diff]
	tool = vimdiff
[difftool]
	prompt = false
[rebase]
	updateRefs = true
[credential]
	helper = cache --timeout 7200
	helper = oauth
[alias]
	pushf = push -f --force-with-lease --force-if-includes
